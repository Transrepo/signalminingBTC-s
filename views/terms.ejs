    document.addEventListener("DOMContentLoaded", function() {
        // Function to generate a random name
        function generateRandomName() {
            const names = ["Tony", "Alex", "Emily", "Chris", "Sophia", "Michael", "Olivia", "William", "Ava", "James", "Grace", "Daniel", "Emma", "Joseph", "Mia", "David", "Ella", "Andrew", "Amelia", "Benjamin", "Sophie", "Samuel", "Lily", "Matthew", "Chloe", "Christopher", "Zoe", "John", "Aiden", "Natalie", "Jackson", "Abigail", "Nicholas", "Charlotte", "Ethan", "Madison", "Daniel", "Liam", "Sophie", "Ryan", "Grace", "Nathan", "Avery", "Gabriel", "Hannah", "Dylan", "Evelyn", "Caleb", "Scarlett"];
            const randomIndex = Math.floor(Math.random() * names.length);
            return names[randomIndex];
        }

        // Function to generate a random country
        function generateRandomCountry() {
            const countries = ["USA", "Canada", "UK", "Australia", "Germany", "France", "Italy", "Spain", "Japan", "Brazil", "Mexico", "Argentina", "Russia", "South Korea", "Turkey", "South Africa", "Nigeria", "Egypt", "Kenya", "Saudi Arabia", "Thailand", "Vietnam", "Indonesia", "Malaysia", "Singapore", "New Zealand", "Sweden", "Norway", "Denmark", "Finland", "Netherlands", "Belgium", "Switzerland", "Austria", "Greece", "Portugal", "Ireland", "Poland", "Hungary", "Romania", "Bulgaria", "Croatia", "Serbia", "Slovakia", "Slovenia"];
            const randomIndex = Math.floor(Math.random() * countries.length);
            return countries[randomIndex];
        }

        // Function to generate a random withdrawal amount
        function generateRandomAmount() {
            return (Math.floor(Math.random() * 144000) + 40000); // Random amount between $9387 and $473120
        }

        // Function to display the pop-up
        function displayPopup() {
            const popupContainer = document.getElementById("custom-popupContainer");
            const popupUsername = document.getElementById("popup-username");
            const popupCountry = document.getElementById("popup-country");
            const popupAmount = document.getElementById("popup-amount");

            // Generate random name, country, and amount
            const username = generateRandomName();
            const country = generateRandomCountry();
            const amount = generateRandomAmount();

            // Set content and display the pop-up
            popupUsername.textContent = username;
            popupCountry.textContent = country;
            popupAmount.textContent = amount;
            popupContainer.style.display = "flex";

            // Automatically close the pop-up after 3 seconds
            setTimeout(function() {
                popupContainer.style.display = "none";

                // Display a new pop-up after 2 seconds
                setTimeout(displayPopup, 2000);
            }, 5000);
        }

        // Start the pop-up display loop
        displayPopup();
    });
    </script>


