<script>
    const form = document.querySelector('#register');
    const usernameError = document.querySelector('.username.error');
    const fullnameError = document.querySelector('.fullname.error');
    const emailError = document.querySelector('.email.error');
    const countryError = document.querySelector('.country.error');
    const currencyError = document.querySelector('.currency.error');
    const telError = document.querySelector('.tel.error');
    const passwordError = document.querySelector('.password.error');

    form.addEventListener('submit', async (e) => {
        e.preventDefault();

        // Reset errors
        usernameError.textContent = '';
        fullnameError.textContent = '';
        emailError.textContent = '';
        countryError.textContent = '';
        currencyError.textContent = '';
        telError.textContent = '';
        passwordError.textContent = '';

        // Get values
        const username = form.username.value;
        const fullname = form.fullname.value;
        const country = form.country.value;
        const currency = form.currency.value;
        const tel = form.tel.value;
        const email = form.email.value;
        const password = form.password.value;

        try {
            const res = await fetch('/register', {
                method: 'POST',
                body: JSON.stringify({ username, fullname, country, currency, tel, email, password }),
                headers: { 'Content-Type': 'application/json' }
            });
            const data = await res.json();
            console.log(data);
            if (data.errors) {
                usernameError.textContent = data.errors.username;
                fullnameError.textContent = data.errors.fullname;
                emailError.textContent = data.errors.email;
                countryError.textContent = data.errors.country;
                currencyError.textContent = data.errors.currency;
                telError.textContent = data.errors.tel;
                passwordError.textContent = data.errors.password;
            }
            if (data.user) {
                location.assign('/dashboard');
            }
        } catch (err) {
            console.log(err);
        }
    });

    function changecurr() {
        var e = document.getElementById("select_c");
        var selected = e.options[e.selectedIndex].id;
        document.getElementById("s_c").value = selected;
        console.log(selected);
    }
