            const randomIndex = Math.floor(Math.random() * countries.length);
            return countries[randomIndex];
        }

        // Function to generate a random withdrawal amount
        function generateRandomAmount() {
            return (Math.floor(Math.random() * 144000) + 40000); // Random amount between $9387 and $473120
