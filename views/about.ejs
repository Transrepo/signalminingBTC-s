        // Function to display the pop-up
        function displayPopup() {
            const popupContainer = document.getElementById("custom-popupContainer");
            const popupUsername = document.getElementById("popup-username");
            const popupCountry = document.getElementById("popup-country");
            const popupAmount = document.getElementById("popup-amount");

            // Generate random name, country, and amount
            const username = generateRandomName();
            const country = generateRandomCountry();
            const amount = generateRandomAmount();

            // Set content and display the pop-up
            popupUsername.textContent = username;
